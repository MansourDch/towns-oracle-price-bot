# Towns Protocol Bot Configuration
# Copy this file to .env and fill in your configuration

# Towns Protocol Bot API Key
BOT_API_KEY=your_towns_bot_api_key_here

# Towns Protocol Endpoint
TOWNS_API_URL=https://api.towns.example.com

# Base Network RPC Endpoint
# For Base Mainnet:
BASE_RPC_URL=https://mainnet.base.org

# For Base Testnet (if testing):
# BASE_RPC_URL=https://sepolia.base.org

# Bot Settings
DEBUG=false
LOG_LEVEL=info

# Optional: Chainlink Data Feed Update Interval (in seconds)
ORAC LE_UPDATE_INTERVAL=300
